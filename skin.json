{
	"name": "Dusk",
	"version": "4.3.6",
	"author": [
		"Gregory S. Hayes",
		"Becca Wei",
		"Jack Phoenix",
		"Samantha Nguyen"
	],
	"url": "https://www.mediawiki.org/wiki/Skin:Dusk",
	"descriptionmsg": "dusk-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"ValidSkinNames": {
		"dusk": {
			"displayname": "Dusk",
			"class": "SkinTemplate",
			"args": [ {
				"name": "dusk",
				"bodyOnly": true,
				"template": "DuskTemplate",
				"styles": [
					"skins.dusk"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"SkinDusk": [
			"i18n"
		]
	},
	"ThemeModules": {
		"dusk": [ "green" ]
	},
	"AutoloadClasses": {
		"DuskTemplate": "includes/DuskTemplate.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Dusk"
	},
	"ResourceModules": {
		"skins.dusk": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"content-links": true,
				"content-links-external": true,
				"content-media": true,
				"interface-message-box": true,
				"interface-category": true,
				"content-tables": true,
				"i18n-ordered-lists": true,
				"i18n-all-lists-margins": true,
				"i18n-headings": true,
				"elements": true,
				"interface": true,
				"logo": true
			},
			"styles": {
				"resources/css/main.css": {
					"media": "screen"
				},
				"resources/css/print.css": {
					"media": "print"
				}
			}
		},
		"themeloader.skins.dusk.green": {
			"styles": {
				"resources/themes/green/green.css": {
					"media": "screen"
				}
			}
		}
	},
	"ResourceModuleSkinStyles": {
		"dusk": {
			"+ext.comments.css": "resources/css/extensions/ext.comments.css",
			"+ext.fanBoxes": "resources/css/extensions/ext.fanBoxes.css",
			"+ext.fanBoxes.createform": "resources/css/extensions/ext.fanBoxes.createform.css",
			"+ext.pollNY.css": "resources/css/extensions/ext.pollNY.css",
			"+ext.problemReports": "resources/css/extensions/ext.problemReports.css",
			"+ext.problemReports.special": "resources/css/extensions/ext.problemReports.special.css",
			"+ext.socialprofile.special.givegift.css": "resources/css/extensions/ext.socialprofile.special.givegift.css"
		}
	},
	"manifest_version": 1
